{
  "author": "Vyacheslav Buglov <v.a.buglov@gmail.com> (https://buglov.ru/)",
  "contributors": [
    "Vyacheslav Buglov <v.a.buglov@gmail.com> (https://buglov.ru/)"
  ],
  "name": "@dev-bi/rct_utils",
  "description": "A practical functional library for JavaScript programmers.",
  "keywords": [
    "rct_utils",
    "functional",
    "utils",
    "utilities",
    "toolkit",
    "fp",
    "tacit",
    "point-free",
    "curried",
    "pure",
    "fantasy-land"
  ],
  "sideEffects": false,
  "version": "0.0.6",
  "license": "MIT",
  "scripts": {
    "predeploy": "npm-run-all build:docs babel git.add",
    "deploy": "git commit -m",
    "postdeploy": "npm-run-all git.push npm.publish",
    "build:docs": "jsdoc -c jsdoc.json",
    "watch:doc-dist": "npx nodemon --exec 'jsdoc -c jsdoc.json' --watch dist",
    "watch:doc-output": "npx livereload build:docs",
    "watch:docs": "npm run watch:doc-output & npm run watch:doc-dist",
    "babel": "npx babel dist --out-dir src",
    "git.add": "git add .",
    "git.commit": "git commit -m",
    "git.push": "git push --all",
    "npm.publish": "npm publish"
  },
  "files": [
    "docs",
    "src",
    "dist"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "graphql-request": "^2.0.0",
    "js-cookie": "^2.2.1",
    "mobx-react": "^6.2.5",
    "mobx-react-lite": "^2.0.7",
    "polished": "^3.6.5",
    "ramda": "^0.27.1",
    "store": "^2.0.12",
    "styled-components": "^5.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vbuglov/coreJsDevbi.git"
  },
  "bugs": {
    "url": "https://github.com/vbuglov/coreJsDevbi/issues"
  },
  "homepage": "https://github.com/vbuglov/coreJsDevbi#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/plugin-transform-exponentiation-operator": "^7.12.1",
    "@babel/plugin-transform-sticky-regex": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "better-docs": "^2.3.2",
    "jest": "^26.1.0",
    "jsdoc": "^3.6.6",
    "jsdoc-mermaid": "^1.0.0",
    "lint-staged": "^10.2.11",
    "livereload": "^0.9.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  },
  "module": "src/index",
  "main": "src/index",
  "typings": "dist/index",
  "directories": {
    "doc": "docs"
  }
}
